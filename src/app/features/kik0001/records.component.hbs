<div class="row">
  <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">

    <div>
      <ul class="list-group">
        {{#each records}}
        <li class="list-group-item">

          {{#ifc contract "activity"}}
            {{> activity.partial this}}
          {{/ifc}}

          {{#ifc contract "document"}}
             {{> document.partial this}}
          {{/ifc}}
          
        </li>
        {{/each}}
      </ul>
    </div>
   
  </div>
</div>