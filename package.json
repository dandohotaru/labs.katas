{
  "name": "labs.katas",
  "version": "0.1.0",
  "description": "individual training exercises in programming and other software crafts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dandohotaru/labs.katas.git"
  },
  "keywords": [
    "labs",
    "crafts",
    "kata"
  ],
  "author": "Dan Dohotaru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dandohotaru/labs.katas/issues"
  },
  "homepage": "https://github.com/dandohotaru/labs.katas#readme",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "bundle": "npm run clean && webpack --progress --env.platform=web",
    "bundle:dev": "npm run bundle -- --env.target=development",
    "bundle:prod": "npm run bundle -- --env.target=production",
    "serve": "npm run clean && webpack-dev-server --inline --env.platform=web",
    "serve:dev": "npm run serve -- --env.target=development",
    "serve:prod": "npm run serve -- --env.target=production",
    "start": "npm run serve:dev"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
