{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "build",
      "program": "${workspaceFolder}/node_modules/.bin/webpack-cli",
      "args": [
        "--config",
        "webpack.dev.js"
      ],
      "autoAttachChildProcesses": true,
      "stopOnEntry": true,
      "outFiles": [
        "${workspaceFolder}/dist/app.bundle.js"
      ],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "webpack-dev-server",
      "program": "${workspaceRoot}/node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "cwd": "${workspaceFolder}",
      "args": [
        "--config",
        "webpack.dev.js"
      ],
    }
  ],
  "compounds": []
}