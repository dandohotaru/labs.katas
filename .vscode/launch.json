{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "build",
      "program": "${workspaceFolder}/node_modules/webpack-cli/bin/webpack.js",
      "args": [
        "--config",
        "webpack.dev.js"
      ],
      "stopOnEntry": true,
      "outFiles": [
        "${workspaceFolder}/dist/*"
      ],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "start",
      "program": "${workspaceFolder}/node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "args": [
        "--config",
        "webpack.dev.js"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/*"
      ],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "stopOnEntry": false,
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/../*",
        "webpack:///./*": "${webRoot}/../*",
        "webpack:///src/*": "${webRoot}/../src/*",
        "webpack:///./~/*": "${webRoot}/../node_modules/*"
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "lite",
      "url": "http://localhost:3001",
      "port": 9222,
      "webRoot": "${workspaceFolder}/dist",
      "sourceMaps": true,
      "disableNetworkCache": true,
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/../*",
        "webpack:///./*": "${webRoot}/../*",
        "webpack:///src/*": "${webRoot}/../src/*",
        "webpack:///./~/*": "${webRoot}/../node_modules/*"
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}/dist",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///*": "${workspaceRoot}/../*",
        "webpack:///./*": "${workspaceRoot}/../*",
        "webpack:///src/*": "${workspaceRoot}/../src/*",
        "webpack:///./~/*": "${workspaceRoot}/../node_modules/*"
      }
    }
  ],
  "compounds": []
}